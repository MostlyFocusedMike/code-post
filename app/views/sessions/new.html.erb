<h1>LOG IN</h1>
<% if flash[:log_in_warning] %>
  <div class="notice"><%= flash[:log_in_warning]  %></div>
<% end %>

<%= form_tag login_path do %>
  <%= label_tag :username %>
  <%= text_field_tag :username %><br />
  <%= label_tag :password %>
  <%= password_field_tag :password %><br />

  <%= submit_tag "log in" %>
<% end %>
