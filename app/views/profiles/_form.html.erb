<h1>NEW FORM HERE </h1>
<%= form_for @profile do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, placeholder: "Your username here"%><br />

  <%= f.label :email %>
  <%= f.email_field :email, placeholder: "user@example.com" %><br />

  <%= f.label :password %>
  <%= f.password_field :password %><br />

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %><br />

  <%= f.submit %>
<% end %>
